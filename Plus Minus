function plusMinus(arr) {
    let positive = (arr.filter(number => number > 0).length / arr.length).toFixed(6)
    let negative = (arr.filter(number => number < 0).length / arr.length).toFixed(6)
    let zeros = (arr.filter(number => number == 0).length / arr.length).toFixed(6)
    
    console.log(positive + '\n' + negative + '\n' + zeros)
    
}
